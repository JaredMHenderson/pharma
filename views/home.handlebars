


<div class="container">
	<div class="row">
		<div class="col-lg-8">
			<h1 class="headline">Welcome</h1>
		</div>
		<div class="col-lg-4">
			<a class="btn btn-outline-dark btn-lg" id="seconBtn" href="newPatientForm" role="button">Add New Patient</a>
			<a class="btn btn-outline-dark btn-lg" id="seconBtn" href="prescription" role="button">Add New Prescription</a>
		</div>
	</div>
		<hr>
	<div class="row">
		<h3 class="subHead">Recent Patients</h3>
    </div>
	


<div class="row">

  <table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">DOB</th>
      <th scope="col">Prescription</th>
    </tr>
  </thead>

  <tbody>
      {{#each patients}}
    <tr>
      <th scope="row">{{this.name}}</th>
      <td>{{this.DOB}}</td>
      <td>{{this.prescriptions}}</td>
    </tr>
    {{/each}}
   
  </tbody>
</table>
 
</div>
</div>



